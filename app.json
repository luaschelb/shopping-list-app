{
  "expo": {
    "name": "Lista de Compras",
    "slug": "shopping-list-app",
    "version": "1.0.0",
    "icon": "./assets/images/icon-light.png",
    "orientation": "portrait",
    "scheme": "shoppinglistapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.luaschelb.shoppinglistapp"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/icon-light.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "19febd6f-7c03-4b38-a9da-efe2e922cce4"
      }
    }
  }
}
